{{ $.Store.Set "i" 1001 }}

{{ $.Store.Add "index" slice}}
{{ range .Site.RegularPages }}
    {{ $.Store.Add "index" (dict 
        "id" ($.Store.Get "i")
        "title" .Title 
        "url" .RelPermalink 
        "description" .Params.description
        "keywords" .Params.keywords
        "photo" .Params.photo
        "alt" .Params.alt
        "numero" .Params.numero
        )
    }}
    {{ $.Store.Add "i" 1}}
{{ end }}
{{ $.Store.Get "index" | jsonify}}